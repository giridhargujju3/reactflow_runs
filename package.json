{
  "name": "pd-visualization",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:frontend": "npm run start --workspace=frontend",
    "dev:backend": "npm run start --workspace=backend",
    "frontend": "npm run start --workspace=frontend",
    "backend": "npm run start --workspace=backend",
    "install:all": "npm install"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
